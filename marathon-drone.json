{
  "container": {
    "docker": {
      "forcePullImage": false, 
      "image": "drone/drone:0.4", 
      "network": "BRIDGE", 
      "parameters": [], 
      "portMappings": [
        {
          "containerPort": 8000, 
          "hostPort": 0
        }
      ], 
      "privileged": false
    }, 
    "type": "DOCKER", 
    "volumes": [
      {
        "containerPath": "/var/run/docker.sock", 
        "hostPath": "/var/run/docker.sock", 
        "mode": "RW"
      }, 
      {
        "containerPath": "/var/lib/drone", 
        "hostPath": "/var/lib/drone", 
        "mode": "RW"
      }
    ]
  }, 
  "cpus": 1, 
  "env": {
    "DEBUG": "false", 
    "PLUGIN_FILTER": "plugins/*", 
    "REMOTE_CONFIG": "https://github.com?client_id=b0b3b55964632fb6e3ba&client_secret=23c5ebf6e5b5cf51836033a5e30315e2de48192e", 
    "REMOTE_DRIVER": "github"
  }, 
  "healthChecks": [
    {
      "gracePeriodSeconds": 300, 
      "ignoreHttp1xx": false, 
      "intervalSeconds": 60, 
      "maxConsecutiveFailures": 3, 
      "path": "/login", 
      "portIndex": 0, 
      "protocol": "HTTP", 
      "timeoutSeconds": 20
    }
  ], 
  "id": "cicd-drone", 
  "instances": 1, 
  "labels": {
    "traefik.frontend.passHostHeader": "true"
  }, 
  "mem": 64
}
